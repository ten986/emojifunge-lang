{
  "name": "emojifunge-lang",
  "version": "1.0.0",
  "description": "emojifunge",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "run-s lint test:*",
    "fix": "run-p fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext js,ts .",
    "lint:prettier": "prettier -l \"**/*.{json,md,yml}\"",
    "lint:type": "tsc --noEmit",
    "test:unit": "jest",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev:test": "cat src/sample/input | (./node_modules/.bin/ts-node -r tsconfig-paths/register src/index.ts src/sample/hello)",
    "dev:watch": "ts-node-dev --respawn src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ten986/emojifunge-lang.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ten986/emojifunge-lang/issues"
  },
  "homepage": "https://github.com/ten986/emojifunge-lang#readme",
  "devDependencies": {
    "@types/jest": "26.0.20",
    "@types/node": "14.14.31",
    "@types/node-emoji": "1.8.1",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "eslint": "7.21.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "5.1.1",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "node-emoji": "1.10.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "26.5.2",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.6",
    "typescript": "^4.2.2",
    "typescript-eslint-parser": "22.0.0"
  },
  "engines": {
    "node": ">= 12",
    "npm": ">= 6.12"
  },
  "dependencies": {
    "@types/mathjs": "6.0.11",
    "mathjs": "9.2.0"
  }
}
